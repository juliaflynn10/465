<p id="notice"><%= notice %></p>

<h1>Listing Seating Charts</h1>

<table>
  <thead>
    <tr>
      <th>Course</th>
      <th>User</th>
      <th>Seat count</th>
      <th>Course</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @seating_charts.each do |seating_chart| %>
      <tr>
        <td><%= seating_chart.course_id %></td>
        <td><%= seating_chart.user_id %></td>
        <td><%= seating_chart.seat_count %></td>
        <td><%= seating_chart.course %></td>
        <td><%= link_to 'Show', seating_chart %></td>
        <td><%= link_to 'Edit', edit_seating_chart_path(seating_chart) %></td>
        <td><%= link_to 'Destroy', seating_chart, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Seating chart', new_seating_chart_path %>
