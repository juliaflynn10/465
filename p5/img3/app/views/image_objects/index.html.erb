<h1>Listing image_objects</h1>

<table>
  <thead>
    <tr>
      <th>Filename</th>
      <th>Public</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @image_objects.each do |image_object| %>
      <tr>
        <td><%= image_object.filename %></td>
        <td><%= image_object.public %></td>
        <td><%= link_to 'Show', image_object %></td>
        <td><%= link_to 'Edit', edit_image_object_path(image_object) %></td>
        <td><%= link_to 'Destroy', image_object, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Image object', new_image_object_path %>
